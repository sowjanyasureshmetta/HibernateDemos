<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "org.gelm.entities.Agent" table = "AGENT">
      
      <meta attribute = "class-description">
         This class contains the Agent information. 
      </meta>
      
      <id name = "agentId" type = "string" column = "agent_id">
      </id>
      <property name = "agentName" column = "agent_name" type = "string"/>
      <property name = "agentBranch" column = "agent_branch" type = "string"/>
      
   </class>
</hibernate-mapping>